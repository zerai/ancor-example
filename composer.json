{
  "name": "zerai/ancor-example",
  "type": "project",
  "description": "Provides an ancor example.",
  "homepage": "https://github.com/zerai/ancor-example",
  "license": "MIT",
  "authors": [
    {
      "name": "Zerai Teclai",
      "email": "teclaizerai@gmail.com.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.13.3",
    "ergebnis/license": "^1.1.0",
    "ergebnis/php-cs-fixer-config": "~2.13.0",
    "ergebnis/phpstan-rules": "~0.15.3",
    "ergebnis/test-util": "^1.4.0",
    "infection/infection": "~0.18.2",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "~0.12.81",
    "phpstan/phpstan-deprecation-rules": "~0.12.6",
    "phpstan/phpstan-phpunit": "~0.12.18",
    "phpstan/phpstan-strict-rules": "~0.12.9",
    "phpunit/phpunit": "~9.2.6",
    "psalm/plugin-phpunit": "~0.15.1",
    "vimeo/psalm": "^4.6.2"
  },
  "config": {
    "platform": {
      "php": "7.4.18"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Library\\": "src/",
      "TaskManagement\\": "task-management/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Library\\Test\\": "test/",
      "TaskManagement\\Tests\\": "task-management/tests/"
    }
  },
  "support": {
    "issues": "https://github.com/zerai/ancor-example/issues",
    "source": "https://github.com/zerai/ancor-example"
  }
}
